
SQL> CREATE TABLE Employees( empid number (4) , ename varchar (30) , address varchar
  2  (50) , salary number (20) , doj date , deptid number (5) );

Table created.

SQL>
SQL> INSERT INTO Employees VALUES
  2  (121,'SUBIKSHA','COIMBATORE',50000,'03FEB2002',567) ;

1 row created.

SQL> INSERT INTO Employees VALUES
  2  (122,'KAVIYA','COIMBATORE',100000,'24MAR2003',566) ;

1 row created.

SQL> INSERT INTO Employees VALUES
  2  (123,'DHARSAN','SALEM',80000,'03OCT2002',588) ;

1 row created.

SQL> INSERT INTO Employees VALUES
  2  (124,'KATHIR','DINDUGUL',90000,'27MAY2002',600) ;

1 row created.

SQL> INSERT INTO Employees VALUES (125,'SIVA','THENI',60000,'24JUN2002',677) ;

1 row created.

SQL> INSERT INTO Employees VALUES
  2  (126,'KARISHMA','CHENNAI',70000,'18SEP2002',699) ;

