SQL> CREATE TABLE Employee2 ( empid number (4) , ename varchar (30) , address varchar
  2  (50) , salary number (20) , doj date , deptid number (5) ) ;

Table created.

SQL> INSERT INTO Employee2 VALUES
  2  (121,'SUBIKSHA','COIMBATORE',50000,'03FEB2002',567) ;

1 row created.

SQL> INSERT INTO Employee2 VALUES
  2  (127,'PRITHI','VIRUDUNAGAR',80000,'28NOV2001',566) ;

